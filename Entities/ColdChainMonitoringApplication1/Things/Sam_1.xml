                                        <![CDATA[
                                        // result: INFOTABLE dataShape: ""
                                        let reqEntry = Things["ColdChainStream_1"].GetStreamEntry({
                                        	streamEntryId: Id/* STRING */
                                        });
                                        
                                        Things["ColdChainStream_1"].DeleteStreamEntry({
                                        	streamEntryId:Id/* STRING */
                                        });
                                        
                                        message=reqEntry.AlertMessage;
                                        time=reqEntry.AlertTime;
                                        truckId=reqEntry.TruckId;
                                        name=reqEntry.Truck_Name;
                                        // CreateInfoTableFromDataShape(infoTableName:STRING("InfoTable"), dataShapeName:STRING):INFOTABLE(AlertDS_1)
                                        let table = Resources["InfoTableFunctions"].CreateInfoTableFromDataShape({
                                            infoTableName: "InfoTable",
                                            dataShapeName: "AlertDS_1"
                                        });
                                        
                                        
                                        // AlertDS_1 entry object
                                        let newEntry = {
                                            AlertMessage: message,// STRING
                                            AlertTime: time,// DATETIME
                                            IsAcknowledged: true,// BOOLEAN
                                            Message: Comment,// STRING
                                            TruckId:truckId,
                                            Truck_Name:name
                                        };
                                        table.AddRow(newEntry);
                                        
                                        Things["ColdChainAcknowledgedStream"].AddStreamEntry({
                                        	sourceType: undefined /* STRING */,
                                        	values: table /* INFOTABLE */,
                                        	location: undefined /* LOCATION */,
                                        	source: undefined /* STRING */,
                                        	timestamp: undefined /* DATETIME */,
                                        	tags: undefined /* TAGS */
